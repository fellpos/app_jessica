{
  "expo": {
    "name": "app-jessica",
    "slug": "app-jessica",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-tcc.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-tcc.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fleups.appjessica"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-tcc.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-tcc.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundImage": "./assets/images/fundo.png"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7860e273-1120-4bf8-81dd-6790c8989f06"
      }
    }
  }
}
